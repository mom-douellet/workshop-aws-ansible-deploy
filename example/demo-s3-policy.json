{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3ListTalendSources",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::ca.momentumtechnologies.aws.workshop.xxxx"
            ]
        },
        {
            "Sid": "S3GetTalendSources",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject"
            ],
            "Resource": [
                "arn:aws:s3:::ca.momentumtechnologies.aws.workshop.xxxx/*"
            ]
        }
    ]
}
